!-- Xft settings -- ! you dont even need this probably 

Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault

! transparency ( 0.1 to 0.9 values)
st.alpha: 0.8 

! inner padding
st.borderpx:          15
st.font:        CaskaydiaCove Nerd Font Mono :pixelsize=15:antialias=true:autohint=true 

*background: #1E1E28
*foreground: #DADAE8

! Black + dark black
*color0: #6E6C7E
*color8: #6E6C7E

! Red
*color1: #E38C8F
*color9: #E38C8F

! Green
*color2: #B1E3AD
*color10: #B1E3AD

! Yellow
*color3: #EBDDAA
*color11:  #EBDDAA

! Blue
*color4: #A4B9EF
*color12: #A4B9EF

! Magenta
*color5: #C6AAE8
*color13: #C6AAE8

! Pink
*color6: #E5B4E2
*color14: #E5B4E2

! Whites
*color7: #DADAE8
*color15: #DADAE8
